function numtomonth()
{
 #read month
 #i=1
while(`test $1`)
do
case $10 in
1) echo jan ;;
2) echo feb ;;
3) echo mar ;;
4) echo apr ;;
5) echo may ;;
6) echo jun ;;
7) echo jul ;;
8) echo aug ;;
9) echo sep ;;
10) echo oct ;;
11) echo nov ;;
12) echo dec ;;

esac
shift
done

}
numtomonth 1 2 3 4 5 6
numtomonth 7 8 9 10 11 12

